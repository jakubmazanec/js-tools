{
  "name": "@jakubmazanec/changesets-changelog",
  "version": "1.0.7",
  "description": "Changelog for projects using Changesets.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/js-tools.git",
    "directory": "packages/changesets-changelog"
  },
  "license": "MIT",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": "./dist/main.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "del-cli dist tsconfig.tsbuildinfo && swc src -d dist && tsc",
    "clean": "del-cli coverage dist *.tsbuildinfo",
    "format": "prettier . --write --ignore-unknown --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "test": "del-cli coverage && jest --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@changesets/get-github-info": "0.5.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@changesets/parse": "^0.3.15",
    "@changesets/types": "^5.2.0",
    "@types/node": "^20.8.5"
  },
  "packageManager": "npm@10.1.0",
  "engines": {
    "node": "^20.8.0"
  }
}
