{
  "name": "@jakubmazanec/changesets-changelog",
  "version": "0.0.0",
  "description": "Changelog for projects using Changesets.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/js-tools.git",
    "directory": "packages/changesets-changelog"
  },
  "license": "MIT",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": "./dist/changesets-changelog.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "swc src -d dist && tsc",
    "clean": "del-cli dist *.tsbuildinfo",
    "format": "prettier . --write --ignore-unknown --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@changesets/get-github-info": "0.5.1",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@changesets/parse": "^0.3.13",
    "@changesets/types": "^5.0.0",
    "@types/node": "^16.11.43"
  },
  "peerDependencies": {
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "packageManager": "npm@8.11.0",
  "engines": {
    "node": ">=16.15.0"
  }
}
