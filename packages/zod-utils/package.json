{
  "name": "@jakubmazanec/zod-utils",
  "version": "1.0.2",
  "description": "Collection of Zod schemas and utilities.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/js-tools.git",
    "directory": "packages/zod-utils"
  },
  "license": "LGPL-3.0-only",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/main.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "del-cli dist tsconfig.tsbuildinfo && swc src -d dist && tsc",
    "clean": "del-cli coverage dist *.tsbuildinfo",
    "document": "del-cli docs && typedoc --options typedoc.config.cjs",
    "format": "prettier . --write --ignore-unknown --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "test": "del-cli coverage && cross-env NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@jakubmazanec/ts-utils": "^2.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^20.8.5",
    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "zod": "^3.19.1"
  },
  "packageManager": "npm@10.1.0",
  "engines": {
    "node": "^20.8.0"
  }
}
