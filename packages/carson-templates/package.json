{
  "name": "@jakubmazanec/carson-templates",
  "version": "1.0.0",
  "description": "Collection of Carson templates.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/js-tools.git",
    "directory": "packages/carson-templates"
  },
  "license": "MIT",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/main.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "templates"
  ],
  "scripts": {
    "build": "del-cli dist tsconfig.tsbuildinfo && swc src -d dist && tsc",
    "clean": "del-cli coverage dist *.tsbuildinfo",
    "document": "del-cli docs && typedoc --options typedoc.config.cjs",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "test": "del-cli coverage && cross-env NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "semver": "^7.3.6"
  },
  "devDependencies": {
    "@jakubmazanec/carson": "^0.2.0",
    "@jakubmazanec/fs-utils": "^0.1.0",
    "@jest/globals": "^29.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.8.5",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "^5.1.3",
    "zen-observable": "^0.10.0"
  },
  "peerDependencies": {
    "@jakubmazanec/carson": "^0.2.0"
  },
  "packageManager": "npm@10.1.0",
  "engines": {
    "node": "^20.8.0"
  }
}
