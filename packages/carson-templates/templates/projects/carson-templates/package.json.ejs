---
to: 'package.json'
strategy: merge
---
<%
let developmentDependencies = getDependencies(
  [
    '@types/node',
    '@vitest/coverage-v8',
    'del-cli',
    'esbuild',
    'eslint',
    'prettier',
    'prettier-plugin-packagejson',
    'typedoc',
    'typedoc-plugin-markdown',
    'typescript',
    'vitest',
  ],
  workspace,
);
let packageJson = {
  repository: {
    type: 'git',
    directory: project.relativePath,
  },
  type: 'module',
  exports: {
    '.': './dist/main.js',
    './package.json': './package.json',
  },
  files: ['dist', 'src', 'templates'],
  scripts: {
    build: `del-cli dist tsconfig.tsbuildinfo && ${BUILD_TO_ESM_COMMAND} && tsc`,
    clean: 'del-cli coverage dist *.tsbuildinfo',
    document: 'del-cli docs && typedoc --options typedoc.config.cjs',
      format: FORMAT_COMMAND,
    lint: 'eslint .',
    test: `del-cli coverage && ${TEST_COMMAND}`,
    typecheck: 'tsc --project tsconfig.typecheck.json',
  },
  devDependencies: developmentDependencies,
  packageManager: 'npm@10.1.0',
  engines: {
    node: '^20.8.0',
  },
};
-%>
<%- JSON.stringify(packageJson) %>
